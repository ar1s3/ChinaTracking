{
    "name": "phpconsole/phpconsole",
    "description": "A detached logging facility for PHP to aid your daily development routine",
    "keywords": ["php", "console", "phpconsole", "logging", "log", "online", "web", "debug", "debugging"],
    "homepage": "http://phpconsole.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Legierski",
            "email": "peter@legierski.net",
            "homepage": "http://legierski.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "legierski/aes": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "ext-openssl": "Allow AES-256 encryption of your data"
    },
    "autoload": {
        "psr-0": {
            "Phpconsole": "src/"
        },
        "files": [
            "src/functions/p.php"
        ]
    }
}
